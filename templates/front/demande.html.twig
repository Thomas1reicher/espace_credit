    <script type="text/javascript">
    $( document ).ready(function() {
        select = $(".div-type-cred option:selected");
        type = select.html();
       if(type == "PRÊT PERSO"){
            $('.credit-perso-form').show();
            $('.credit-perso-form input').val('');
            $('.credit-perso-form select').val('');
        }
        else if(type == "PRÊT AUTO"){
            $('.credit-auto-form').show();
            $('.credit-auto-form input').val('');
            $('.credit-auto-form select').val('');
        }
        else if(type == "PRÊT MOTO"){
            $('.credit-moto-form').show();
            $('.credit-moto-form input').val('');
            $('.credit-moto-form select').val('');
        }
        else if(type == "PRÊT MOBILITÉ"){
            $('.credit-mobilite-form').show();
            $('.credit-mobilite-form input').val('');
            $('.credit-mobilite-form select').val('');
        }
        else if(type == "PRÊT TRAVAUX"){
            $('.credit-travaux-form').show();
            $('.credit-travaux-form input').val('');
            $('.credit-travaux-form select').val('');
        }
        if(type == "PRÊT AUTO" || type == "PRÊT MOTO"){
            $('.credit-vehicule-form').show();
            $('.credit-vehicule-form input').val('');
            $('.credit-vehicule-form select').val('');
        }
        $("select#demande_etat_civil").change(function() {
         if($(this).val()=="Marié"){
            $('.div-mariee').show();

         }else{
            $('.div-mariee').hide();
            $('.div-mariee input').val('');
            $('.div-mariee select').val('');

         }
       });
    });


    </script>
